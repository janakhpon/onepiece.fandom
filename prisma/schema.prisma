generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/dev.db"
}

model member {
  name       String  @id
  name_jp    String?
  image      String
  origin     String
  age        String
  position   String
  devilfruit String
  summary    String
  crew       String
  crew_img   String
  battling   String?
  avatar     String?
  bounty     Int?
}
